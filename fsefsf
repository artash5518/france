[32m+[m[32m<script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/110/three.min.js"></script>[m
[32m+[m[32m<script src="https://cdn.jsdelivr.net/npm/three/examples/js/loaders/OBJLoader.js"></script>[m
[32m+[m[32m<script src="https://cdn.jsdelivr.net/npm/three/examples/js/loaders/MTLLoader.js"></script>[m
[32m+[m
[32m+[m[32m<script>[m
[32m+[m[32m    // –°–æ–∑–¥–∞–Ω–∏–µ —Å—Ü–µ–Ω—ã, –∫–∞–º–µ—Ä—ã –∏ —Ä–µ–Ω–¥–µ—Ä–µ—Ä–∞[m
[32m+[m[32m    const scene = new THREE.Scene();[m
[32m+[m[32m    const camera = new THREE.PerspectiveCamera(75, window.innerWidth / window.innerHeight, 0.1, 1000);[m
[32m+[m[32m    const renderer = new THREE.WebGLRenderer();[m
[32m+[m[32m    renderer.setSize(window.innerWidth, window.innerHeight);[m
[32m+[m[32m    document.body.appendChild(renderer.domElement);[m
[32m+[m
[32m+[m[32m    // –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Å–≤–µ—Ç–∞ –≤ —Å—Ü–µ–Ω—É[m
[32m+[m[32m    const ambientLight = new THREE.AmbientLight(0x404040); // –º—è–≥–∫–∏–π —Å–≤–µ—Ç[m
[32m+[m[32m    scene.add(ambientLight);[m
[32m+[m
[32m+[m[32m    const directionalLight = new THREE.DirectionalLight(0xffffff, 1); // –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–Ω—ã–π —Å–≤–µ—Ç[m
[32m+[m[32m    directionalLight.position.set(5, 5, 5).normalize(); // –ø–æ–∑–∏—Ü–∏—è –∏—Å—Ç–æ—á–Ω–∏–∫–∞ —Å–≤–µ—Ç–∞[m
[32m+[m[32m    scene.add(directionalLight);[m
[32m+[m
[32m+[m[32m    // –ö–∞–º–µ—Ä–∞[m
[32m+[m[32m    camera.position.set(0, 0, 500);  // –ö–∞–º–µ—Ä–∞ –Ω–∞ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–∏ 5 –µ–¥–∏–Ω–∏—Ü –æ—Ç —Ü–µ–Ω—Ç—Ä–∞[m
[32m+[m[32m    camera.lookAt(0, 0, 0);  // –ö–∞–º–µ—Ä–∞ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∞ –Ω–∞ —Ü–µ–Ω—Ç—Ä —Å—Ü–µ–Ω—ã[m
[32m+[m
[32m+[m[32m    // –ó–∞–≥—Ä—É–∂–∞–µ–º –º–æ–¥–µ–ª—å –∏ –º–∞—Ç–µ—Ä–∏–∞–ª—ã[m
[32m+[m[32m    const mtlLoader = new THREE.MTLLoader();[m
[32m+[m[32m    mtlLoader.load('img/models/arjoc.mtl', (materials) => {[m
[32m+[m[32m        materials.preload();  // –ü—Ä–∏–º–µ–Ω—è–µ–º –º–∞—Ç–µ—Ä–∏–∞–ª—ã[m
[32m+[m[32m        const objLoader = new THREE.OBJLoader();[m
[32m+[m[32m        objLoader.setMaterials(materials);  // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –º–∞—Ç–µ—Ä–∏–∞–ª—ã –¥–ª—è OBJ –º–æ–¥–µ–ª–∏[m
[32m+[m[32m        objLoader.load([m
[32m+[m[32m            'img/models/arjoc.obj',  // –ó–∞–º–µ–Ω–∏—Ç–µ –Ω–∞ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –ø—É—Ç—å –∫ —Ñ–∞–π–ª—É .obj[m
[32m+[m[32m            (object) => {[m
[32m+[m[32m                console.log('Model loaded successfully');[m
[32m+[m[32m                object.traverse((child) => {[m
[32m+[m[32m                    if (child.isMesh) {[m
[32m+[m[32m                        // –ü—Ä–∏–º–µ–Ω—è–µ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –º–∞—Ç–µ—Ä–∏–∞–ª (–µ—Å–ª–∏ —Ç–µ–∫—Å—Ç—É—Ä–∞ –Ω–µ —Ä–∞–±–æ—Ç–∞–µ—Ç)[m
[32m+[m[32m                        child.material = new THREE.MeshStandardMaterial({ color: 0x00ff00 });  // –ó–µ–ª–µ–Ω—ã–π —Ü–≤–µ—Ç[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                scene.add(object); // –î–æ–±–∞–≤–ª—è–µ–º –º–æ–¥–µ–ª—å –≤ —Å—Ü–µ–Ω—É[m
[32m+[m[32m                animate(); // –ó–∞–ø—É—Å–∫ –∞–Ω–∏–º–∞—Ü–∏–∏[m
[32m+[m[32m            },[m
[32m+[m[32m            undefined,  // –ü—Ä–æ–≥—Ä–µ—Å—Å –∑–∞–≥—Ä—É–∑–∫–∏ –º–æ–∂–Ω–æ –æ–±—Ä–∞–±–æ—Ç–∞—Ç—å –∑–¥–µ—Å—å[m
[32m+[m[32m            (error) => {[m
[32m+[m[32m                console.error('An error occurred while loading the model:', error);[m
[32m+[m[32m            }[m
[32m+[m[32m        );[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // –ê–Ω–∏–º–∞—Ü–∏—è –∏ —Ä–µ–Ω–¥–µ—Ä–∏–Ω–≥[m
[32m+[m[32m    function animate() {[m
[32m+[m[32m        requestAnimationFrame(animate);[m
[32m+[m[32m        renderer.render(scene, camera);  // –†–µ–Ω–¥–µ—Ä–∏–Ω–≥ —Å—Ü–µ–Ω—ã[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –æ–∫–Ω–∞[m
[32m+[m[32m    window.addEventListener('resize', () => {[m
[32m+[m[32m        renderer.setSize(window.innerWidth, window.innerHeight);[m
[32m+[m[32m        camera.aspect = window.innerWidth / window.innerHeight;[m
[32m+[m[32m        camera.updateProjectionMatrix();[m
[32m+[m[32m    });[m
[32m+[m[32m</script>[m
